<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <View
        android:id="@+id/divider_me_bar"
        android:layout_width="match_parent"
        android:layout_height="55dp"/>

    <Button
        android:id="@+id/btn_me_bar"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@drawable/selector_me_bar"
        app:layout_constraintTop_toBottomOf="@+id/divider_me_bar"
        />

    <ImageView
        android:id="@+id/img_me_main"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:scaleType="fitCenter"
        android:paddingLeft="20dp"
        android:paddingTop="20dp"
        android:paddingBottom="20dp"
        android:src="@drawable/img_avatar_g"
        app:layout_constraintBottom_toBottomOf="@+id/btn_me_bar"
        app:layout_constraintStart_toStartOf="@id/btn_me_bar"
        app:layout_constraintTop_toTopOf="@+id/btn_me_bar" />

    <TextView
        android:id="@+id/txt_me_main_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:text="我是名字"
        app:layout_constraintBottom_toBottomOf="@+id/img_me_main"
        app:layout_constraintStart_toEndOf="@id/img_me_main"
        app:layout_constraintTop_toTopOf="@+id/img_me_main"
        />

    <ImageView
        android:id="@+id/img_me_child_one"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:src="@drawable/img_avatar_boy1"
        android:scaleType="fitCenter"
        android:focusable="false"
        android:visibility="gone"
        android:paddingRight="20dp"
        android:paddingBottom="15dp"
        app:layout_constraintEnd_toStartOf="@+id/txt_me_editor"
        app:layout_constraintTop_toTopOf="@+id/txt_me_editor"
        app:layout_constraintBottom_toBottomOf="@+id/txt_me_editor"
        />

    <ImageView
        android:id="@+id/img_me_child_two"
        android:layout_width="41dp"
        android:layout_height="wrap_content"
        android:paddingRight="10dp"
        android:paddingBottom="15dp"
        android:focusable="false"
        android:visibility="gone"
        android:src="@drawable/img_avatar_boy2"
        app:layout_constraintEnd_toStartOf="@+id/img_me_child_one"
        app:layout_constraintTop_toTopOf="@+id/img_me_child_one"
        app:layout_constraintBottom_toBottomOf="@+id/img_me_child_one"
        />

    <ImageView
        android:id="@+id/img_me_editor_bar"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@drawable/img_edit_g"
        android:focusable="false"
        android:paddingBottom="20dp"
        android:paddingRight="20dp"
        app:layout_constraintEnd_toEndOf="@id/btn_me_bar"
        app:layout_constraintBottom_toBottomOf="@+id/btn_me_bar"
        />
    <TextView
        android:id="@+id/txt_me_editor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:text="编辑"
        android:paddingBottom="18dp"
        android:paddingRight="5dp"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@id/img_me_editor_bar"
        app:layout_constraintBottom_toBottomOf="@+id/img_me_editor_bar"
        app:layout_constraintTop_toTopOf="@+id/img_me_editor_bar" />

    <View
        android:id="@+id/divider_me_one"
        android:layout_width="match_parent"
        android:background="@color/colorDivider"
        android:layout_height="8dp"
        android:focusable="false"
        app:layout_constraintTop_toBottomOf="@+id/btn_me_bar"
        />

    <Button
        android:id="@+id/btn_me_alert"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:focusable="false"
        android:background="@drawable/selector_me_bar"
        app:layout_constraintTop_toBottomOf="@+id/divider_me_one"
        />

    <TextView
        android:id="@+id/txt_me_alert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:paddingLeft="20dp"
        android:text="提醒"
        android:textSize="18sp"
        android:textColor="@color/colorTextInit"
        app:layout_constraintStart_toStartOf="@id/btn_me_alert"
        app:layout_constraintTop_toTopOf="@+id/btn_me_alert"
        app:layout_constraintBottom_toBottomOf="@+id/btn_me_alert"
        />

    <ImageView
        android:id="@+id/switch_me_alert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingRight="10dp"
        android:src="@drawable/slib_img_on"
        android:background="@color/colorNone"
        app:layout_constraintEnd_toEndOf="@id/btn_me_alert"
        app:layout_constraintTop_toTopOf="@id/btn_me_alert"
        app:layout_constraintBottom_toBottomOf="@id/btn_me_alert"
        />

    <View
        android:id="@+id/divider_me_two"
        android:layout_width="match_parent"
        android:background="@color/colorDivider"
        android:layout_height="2dp"
        app:layout_constraintTop_toBottomOf="@id/btn_me_alert"
        />

    <Button
        android:id="@+id/btn_me_alertType"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/selector_me_bar"
        app:layout_constraintTop_toBottomOf="@+id/divider_me_two"
        />

    <TextView
        android:id="@+id/txt_me_alertType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:paddingLeft="20dp"
        android:text="提醒方式"
        android:textSize="18sp"
        android:textColor="@color/colorTextInit"
        app:layout_constraintStart_toStartOf="@id/btn_me_alert"
        app:layout_constraintTop_toTopOf="@+id/btn_me_alertType"
        app:layout_constraintBottom_toBottomOf="@+id/btn_me_alertType"
        />

    <TextView
        android:id="@+id/txt_me_set_alert"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="振动加提示音"
        android:paddingRight="20dp"
        app:layout_constraintEnd_toEndOf="@id/btn_me_alertType"
        app:layout_constraintTop_toTopOf="@id/btn_me_alertType"
        app:layout_constraintBottom_toBottomOf="@id/btn_me_alertType"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/login_icon_enter"
        android:paddingRight="8dp"
        app:layout_constraintEnd_toEndOf="@id/btn_me_alertType"
        app:layout_constraintTop_toTopOf="@id/btn_me_alertType"
        app:layout_constraintBottom_toBottomOf="@id/btn_me_alertType"
        />

    <View
        android:id="@+id/divider_me_three"
        android:layout_width="match_parent"
        android:background="@color/colorDivider"
        android:layout_height="8dp"
        app:layout_constraintTop_toBottomOf="@id/btn_me_alertType"
        />

    <Button
        android:id="@+id/btn_me_version"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/selector_me_bar"
        app:layout_constraintTop_toBottomOf="@+id/divider_me_three"
        />

    <TextView
        android:id="@+id/txt_me_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="false"
        android:paddingLeft="20dp"
        android:text="版本号"
        android:textSize="18sp"
        android:textColor="@color/colorTextInit"
        app:layout_constraintStart_toStartOf="@id/btn_me_version"
        app:layout_constraintTop_toTopOf="@+id/btn_me_version"
        app:layout_constraintBottom_toBottomOf="@+id/btn_me_version"
        />

    <TextView
        android:id="@+id/txt_me_set_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="V1.6.15"
        android:paddingRight="20dp"
        app:layout_constraintEnd_toEndOf="@id/btn_me_version"
        app:layout_constraintTop_toTopOf="@id/btn_me_version"
        app:layout_constraintBottom_toBottomOf="@id/btn_me_version"
        />

    <View
        android:id="@+id/divider_me_four"
        android:layout_width="match_parent"
        android:background="@color/colorDivider"
        android:layout_height="2dp"
        app:layout_constraintTop_toBottomOf="@id/btn_me_version"
        />





</android.support.constraint.ConstraintLayout>